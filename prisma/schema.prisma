generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model mood {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  date            DateTime @db.Date
  exerciseTrigger String?
  hobbyTrigger    String?
  mealTrigger     Int?
  moodValue       String
  periodTrigger   Boolean?
  sleepTrigger    Int?
  socialTrigger   String?
  suggestions     String
  weatherTrigger  String?
}
