generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model mood {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  date        DateTime @db.Date
  exercise    String
  hobby       String
  meal        String
  moodValue   String
  name        String
  period      Boolean
  sleepEnd    DateTime @db.Date
  sleepHours  Float
  sleepStart  DateTime @db.Date
  social      String
  suggestions String
  weather     String
}
